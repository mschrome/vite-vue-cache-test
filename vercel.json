{
  "functions": {
    "api/blob-upload.js": {
      "maxDuration": 30
    },
    "api/blob-list.js": {
      "maxDuration": 10
    },
    "api/blob-delete.js": {
      "maxDuration": 10
    },
    "api/blob-server-upload.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}